{
  "name": "itscoding/serve-secret",
  "description": "Serve password protected files",
  "type": "craft-plugin",
  "version": "2.4.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "servesecret"
  ],
  "support": {
    "docs": "https://github.com/boscho87/serve-secret/blob/master/README.md",
    "issues": "https://github.com/boscho87/serve-secret/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Simon MÃ¼ller",
      "homepage": "https://itscoding.ch"
    }
  ],
  "require": {
    "craftcms/cms": "^4.0.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8",
    "codacy/coverage": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "itscoding\\servesecret\\": "src/"
    }
  },
  "extra": {
    "name": "ServeSecret",
    "handle": "serve-secret",
    "schemaVersion": "1.0.0",
    "hasCpSettings": false,
    "hasCpSection": false,
    "changelogUrl": "https://raw.githubusercontent.com/boscho87/serve-secret/master/CHANGELOG.md",
    "components": {
      "security": "itscoding\\servesecret\\services\\Security"
    },
    "class": "itscoding\\servesecret\\ServeSecret"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
